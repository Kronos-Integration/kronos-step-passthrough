{
  "name": "kronos-step-passthrough",
  "version": "0.0.0-semantic-release",
  "main": "dist/step-passthrough.js",
  "module": "src/step-passthrough.js",
  "description": "copy trough step implementation",
  "keywords": [
    "kronos-step"
  ],
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "nyc --temp-directory build/nyc ava",
    "doc": "./node_modules/.bin/jsdoc lib/*.js",
    "test": "ava",
    "semantic-release": "semantic-release",
    "precover": "rollup -c tests/rollup.config.js",
    "pretest": "rollup -c tests/rollup.config.js",
    "posttest": "npm run prepare && markdown-doctest",
    "prepare": "rollup -c",
    "docs": "documentation readme src/step-passthrough.js --section=API",
    "lint": "documentation lint src/step-passthrough.js",
    "travis-deploy-once": "travis-deploy-once"
  },
  "dependencies": {
    "kronos-step": "^5.6.1"
  },
  "devDependencies": {
    "semantic-release": "^15.4.0",
    "markdown-doctest": "^0.9.1",
    "nyc": "^11.7.2",
    "ava": "^0.25.0",
    "rollup": "^0.58.2",
    "rollup-plugin-multi-entry": "^2.0.2",
    "documentation": "^6.3.2",
    "travis-deploy-once": "^5.0.0"
  },
  "engines": {
    "node": ">=8.11"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/kronos-step-passthrough.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-step-passthrough/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-step-passthrough#readme",
  "ava": {
    "files": [
      "build/*-test.js"
    ],
    "presets": [
      "env"
    ],
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "build/*-test.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./build/coverage"
  },
  "xo": {
    "space": true
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
