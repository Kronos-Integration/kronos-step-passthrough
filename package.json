{
  "name": "kronos-step-passthrough",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/step-passthrough.js",
  "module": "src/step-passthrough.js",
  "description": "copy trough step implementation",
  "keywords": [
    "kronos-step"
  ],
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "doc": "./node_modules/.bin/jsdoc lib/*.js",
    "docs": "documentation readme src/step-passthrough.js --section=API",
    "lint": "documentation lint src/step-passthrough.js",
    "posttest": "npm run prepare && markdown-doctest",
    "prepare": "rollup -c",
    "test": "ava"
  },
  "dependencies": {
    "kronos-step": "^5.6.1"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^4.0.0",
    "ava": "^2.4.0",
    "builtin-modules": "^3.1.0",
    "c8": "^6.0.1",
    "documentation": "^12.1.4",
    "esm": "^3.2.25",
    "markdown-doctest": "^1.0.0",
    "rollup": "^1.27.7",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-executable": "^1.5.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "semantic-release": "^15.13.31"
  },
  "engines": {
    "node": ">=12.13.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/kronos-step-passthrough.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-step-passthrough/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-step-passthrough#readme",
  "ava": {
    "files": [
      "tests/*-test.mjs"
    ],
    "require": [
      "esm"
    ],
    "babel": false,
    "compileEnhancements": false,
    "extensions": [
      "mjs"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
